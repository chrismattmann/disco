<distributionScenario>
  <totalVolume>20 TB</totalVolume>
  <deliverySchedule>
    <volumePerInterval>1 GB</volumePerInterval>
    <numIntervals valtype="range">
      <range>
        <upperbound inclusive="false">100</upperbound>
      </range>
    </numIntervals>
  </deliverySchedule>
  <numUsers valtype="range">
    <range>
      <lowerbound inclusive="false">10000</lowerbound>
    </range>
  </numUsers>
  <userTypes valtype="range">
    <range>
      <upperbound inclusive="false">10</upperbound>
    </range>
  </userTypes>
  <geographicDistribution>
     <type>WAN</type>
  </geographicDistribution>
  <performancereqs>
    <consistency>4</consistency>
    <dependability>2</dependability>
    <scalability>7</scalability>
    <efficiency>5</efficiency>
  </performancereqs>
  <accessPolicies/>
  <dataTypes>
    <type>data</type>
  </dataTypes>
</distributionScenario>
 