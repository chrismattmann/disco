<distributionScenario>
  <totalVolume valtype="range">
    <range>
      <lowerbound inclusive="false">10 TB</lowerbound>
    </range>
  </totalVolume>
  <deliverySchedule>
    <volumePerInterval>10 TB</volumePerInterval>
    <numIntervals>1</numIntervals>  
  </deliverySchedule>
  <numUsers valtype="range">
    <range>
      <lowerbound inclusive="true">1000</lowerbound>
    </range>
  </numUsers>
  <userTypes>1</userTypes>
  <geographicDistribution>
     <type>LAN</type>
  </geographicDistribution>
  <performancereqs>
    <consistency>8</consistency>
    <dependability>8</dependability>
    <scalability>7</scalability>
    <efficiency>4</efficiency>
  </performancereqs>
  <accessPolicies/>
  <dataTypes>
    <type>data</type>
    <type>metadata</type>
  </dataTypes>
</distributionScenario>
 